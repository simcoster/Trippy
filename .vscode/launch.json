{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: import_json.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/temp_data/import_json.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: replay.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/replay.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: FastAPI (uvicorn)",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload",
                "--host",
                "127.0.0.1",
                "--port",
                "8000"
            ],
            "jinja": true,
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "envFile": "${workspaceFolder}/.env",
            "python": "${workspaceFolder}/.venv/Scripts/python.exe",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }
    ]
}